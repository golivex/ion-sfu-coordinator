                                tee name=venctee
                                vtee. ! queue ! vtenc_h264 ! video/x-h264,chroma-site=mpeg2 ! venctee.
                                atee. ! queue ! faac ! aenctee.

                                qtmux name=savemux ! queue ! filesink location=test.mp4 async=false sync=false
                                venctee. ! queue ! savemux.
                                aenctee. ! queue ! savemux. 



