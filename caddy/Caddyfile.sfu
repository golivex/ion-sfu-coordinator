{
    email {env.CLOUDFLARE_EMAIL}
    http_port 80
    https_port 443
    dynamic_dns {
		provider cloudflare {env.CLOUDFLARE_API_TOKEN}
		domains {
			platoo.in sfu-host sfu-coordinator
		}
	}
}
sfu-host.platoo.in {
    reverse_proxy 127.0.0.1:7002
    tls {
        dns cloudflare {env.CLOUDFLARE_API_TOKEN}
        resolvers 1.1.1.1
    }
}